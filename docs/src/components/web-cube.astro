---
interface Props {
    size?: number;
    disabledPointerEvents?: boolean;
}

const { size = 3, disabledPointerEvents = false } = Astro.props;
---

<web-cube
    size={size}
    disabled-pointer-events={disabledPointerEvents || null}
    class="w-full h-[75dvh] block"></web-cube>
<script>
    import { define } from "web-cube";

    define();
</script>
